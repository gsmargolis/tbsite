<table>
  <tr>
    <th>
      Start
    </th>
    <th>
      Action
    </th>
    <th>
      Result
    </th>
  </tr>
  <% Log.last(25).each do |l| %>
  <tr class="<%= cycle("greenbar", "nogreenbar") %>">
    <td>
     <%= l.startdt %>
    </td>
    <td>
      <%= l.action %>
    </td>
    <td>
      <%= l.result %>
    </td>
  </tr>
  <% end %>
</table>